.Box{
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.swiper{
  width: 100%;
}
swiper image{
  width: 100%;
  height: 350rpx;
}

.inp{
  width: 90%;
  height: 60rpx;
  position: absolute;
  top: 0;
  left: 0;
  padding-left: 30rpx; 
  box-sizing: border-box;
  margin: 20rpx 30rpx;
  border-radius: 30rpx;
  background: rgba(255, 255, 255, 0.86);
}
.gonggao{
  width: 100%;
  height: 60rpx;
  border-bottom: 3rpx solid #f4f4f4;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx;
  box-sizing: border-box;
}
.gonggao image{
  width: 35rpx;
  height: 35rpx;
}
.txt{
  color: red;
  font-size: 26rpx;
  margin-left: -30rpx;
}
.list{
  width: 100%;
  height: 400rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-wrap: wrap;
}
.list view{
    text-align: center;
    width: 140rpx;
}
.list image{
  width: 80rpx;
  height: 80rpx;
}
.tt{
  width: 100%;
  text-align: center;
  color: grey;
  border-bottom:2rpx solid #f4f4f4; 
  padding-bottom: 25rpx;
}
.tuijian{
  width: 100%;
  height: 500rpx;
  display: flex;
  padding:0 20rpx;
  box-sizing: border-box;
  justify-content: space-around;
  align-items: center;
  background: #f4f4f4;
}
.tuijian .box image{
  width: 100%;
  height: 340rpx;
  border-radius: 10rpx 10rpx 0 0;

}
.tuijian .box{
  background: #fff;
  width: 450rpx;
  height: 450rpx;
  margin-left: 20rpx;
  text-align: center;
  border-radius: 10rpx;
}
.tuijian view .uu{
    width: 100%;
    font-size: 26rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    word-break: break-all;
}
.cc{
  text-decoration:line-through;
  color: grey;
  font-size: 26rpx;

}
.wen{
  display: flex;
  justify-content: space-between;
  padding: 20rpx 40rpx;
}
.imger{
  width: 100%;
  height: 200rpx;
  overflow: hidden;
  margin-bottom: 18rpx;
}
.imger image{
  width: 100%;
}
.pintuan{
  padding: 20rpx;
  box-sizing: border-box;
  border-bottom: 25rpx solid #f4f4f4; 
}
.tui{
    margin-left: 30rpx;
}
.tui .btn{
  width:70rpx;
  display: inline;
  background: red;
  color: #fff;
  margin-left: 370rpx;
}
.name{
  margin: 10rpx 0;
  color: gray;
}
.tt1{
    width: 100%;
    height: 60rpx;
    text-align: center;
    color: grey;
    border-bottom:2rpx solid #f4f4f4; 
    padding:20rpx 0 0 25rpx;
}
.fy-vv{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  background: #f4f4f4;
}
.fy-vv .box{
  padding: 10rpx;
  box-sizing: border-box;
  width: 48%;
  background: #fff;
  margin-top: 26rpx;
}
.fy-vv .box image{
  width: 100%;
}
.fy-vv .box .uu{
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    word-break: break-all;
}